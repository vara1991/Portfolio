{% extends 'base.html.twig' %}

{% block title %}CV{% endblock %}

{% block body %}
    {% for item in cv %}
        <object class="cv-pdf" type="application/pdf" data="uploads/cv/{{ item.pdf }}" width="100%" height="100%">
            <a href="uploads/cv/{{ item.pdf }}">Télécharger le fichier</a>
        </object>
    {% endfor %}
{% endblock %}
